if ($("li.closeArc").is(":visible")) {
  $("div#projectContainer").slideToggle();
} else {
  if ($("div#projectContainer").is(":visible")) { $("div#projectContainer").toggle() }
  $("div#projectContainer").html("<%= escape_javascript(render(:partial => "list")) %>").slideToggle();
}
$("div#contentBackground").toggleClass("inactive")
$("li.selectArc, li.closeArc").toggle();
